<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!-- Séquence pour les numéros d'intervention -->
    <record id="sequence_intervention_intervention" model="ir.sequence">
        <field name="name">Intervention Plomberie/Électricité</field>
        <field name="code">intervention.intervention</field>
        <field name="prefix">INT/%(year)s/%(month)s/</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
    
</odoo>
