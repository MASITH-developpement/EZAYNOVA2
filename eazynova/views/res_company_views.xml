<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_eazynova" model="ir.ui.view">
        <field name="name">res.company.form.eazynova</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="EAZYNOVA" name="eazynova">
                    <group>
                        <field name="eazynova_enabled"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
