<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_res_config_settings_eazynova_planning" model="ir.ui.view">
        <field name="name">res.config.settings.eazynova.planning</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="eazynova.view_res_config_settings_eazynova"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='group_ocr']" position="after">
                <group string="Planning global">
                    <field name="eazynova_planning_sync_interval"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
